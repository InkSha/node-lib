{
  "name": "node-lib",
  "version": "1.0.0",
  "description": "Some Develop Library.",
  "author": "InkSha",
  "license": "MIT",
  "main": "dist/index.min.js",
  "module": "dist/index.min.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/InkSha/node-lib.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.min.mjs",
      "require": "./dist/index.min.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "prettier --config .prettierrc.cjs --write",
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "build:format": "prettier --config .prettierrc.cjs --write ./src/** ./*js ./*ts",
    "build:lint": "eslint src/** --fix",
    "build:push": "npm run format && npm run build",
    "publish": "npm publish --access public",
    "commit": "cz-customizable",
    "commit:log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "dev:build": "rollup -c rollup.config.ts --configPlugin typescript --watch",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "dev:js": "tsc",
    "test": "jest --watch",
    "doc": "typedoc --options typedoc.json",
    "format": "npm run build:lint && npm run build:format",
    "start": "node --loader ts-node/esm src/index.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.0.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "commitlint": "^18.0.0",
    "commitlint-config-cz": "^0.13.3",
    "conventional-changelog-cli": "^4.1.0",
    "cz-customizable": "^7.0.0",
    "esbuild": "^0.19.5",
    "eslint": "latest",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2"
  }
}
